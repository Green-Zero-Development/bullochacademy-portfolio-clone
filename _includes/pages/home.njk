<style>
    .home-hero {
        background-image: url('{{ homePage.heroImg }}');
        border-radius: 6px;
    }
    .home-hero-gradient {
        background: rgb(18,71,52);
        background: linear-gradient(180deg, rgba(18,71,52,0) 0%, rgba(18,71,52,1) 100%);
        border-radius: 6px;
    }
</style>

<div class="pt-12 pb-16 px-2 sm:px-6 md:px-16 home-hero-outer">
    <div class="relative max-w-8xl bg-cover bg-center text-white mx-auto p-6 sm:p-10 md:p-20 home-hero" aria-label="{{ homePage.heroImgAlt }}">
        <div class="absolute top-0 left-0 w-full h-full z-10 home-hero-gradient"></div>
        <div class="relative w-full lg:w-2/3 xl:w-9/12 z-20">
            <h1 class="text-4xl sm:text-h1-init leading-tight sm:leading-h1-init pb-2"><i>{{ homePage.heroTitle }}</i></h1>
            <p class="max-w-2xl text-base sm:text-lg-p-init">{{ homePage.heroParagraph }}</p>
            <div class="sm:flex pt-10">
                <a {% if homePage.heroFilledButtonLinkDestination == "A page on this site" %}href='{{ homePage.heroFilledButtonLinkOnSite }}'{% elseif homePage.heroFilledButtonLinkDestination == "Another site" %}href='{{ homePage.heroFilledButtonLinkOffSite }}' target='_blank'{% endif %}>
                    <button class="w-full sm:w-auto mb-6 sm:mb-0 mr-6 gold-button">{{ homePage.heroFilledButtonText }}</button>
                </a>
                <a {% if homePage.heroOutlineButtonLinkDestination == "A page on this site" %}href='{{ homePage.heroOutlineButtonLinkOnSite }}'{% elseif homePage.heroOutlineButtonLinkDestination == "Another site" %}href='{{ homePage.heroOutlineButtonLinkOffSite }}' target='_blank'{% endif %}>
                    <button class="w-full sm:w-auto outline-white-button">{{ homePage.heroOutlineButtonText }}</button>
                </a>
            </div>
        </div>
    </div>
</div>

{% include "global/quick-links-section.njk" %}

<style>
    .after-hero-links-img {
        background-image: url('{{ homePage.afterHeroImg }}');
        border-radius: 6px;
    }

    .after-hero-link:hover h3, .after-hero-link:hover h4 {
        color: #88814B !important;
    }

    .after-hero-line {
        width: 24px;
        height: 2px;
        margin-top: 15px;
        margin-right: 15px;
        transition: .25s;
    }

    .after-hero-link:hover .after-hero-line {
        width: 120px;
        margin-left: -80px;
        transition: .25s;
    }

    .after-hero-link-arrow {
        opacity: 0;
        transform: translateX(-15px);
        transition: .25s;
    }

    .after-hero-link:hover .after-hero-link-arrow  {
        opacity: 1;
        transform: translateX(0px);
        transition: .25s;
    }
</style>

<div class="md:flex items-start lg:items-center max-w-8xl mx-auto pt-16 pb-32 px-2 sm:px-6 md:px-12">
    <div class="bg-cover bg-center w-full md:w-1/2 py-40 md:py-32 lg:py-56 mb-12 md:mb-0 after-hero-links-img" aria-label="{{ homePage.afterHeroImgAlt }}">

    </div>
    <div class="md:pl-8 lg:pl-16 after-hero-links-box">
        {% for item in homePage.afterHeroLinks %}
            <div class="flex after-hero-link">
                <div class="bg-gold-500 after-hero-line"></div>
                <a {% if item.link_to_where == "A page on this site" %}href='{{ item.link }}'{% elseif item.link_to_where == "Another site" %}href='{{ item.link_offsite }}' target='_blank'{% endif %} class="pb-8 after-hero-link-text">
                    <h4 class="text-xs lg:text-sm font-bold text-green-200 uppercase tracking-wider leading-tight">{{ item.subtitle }}</h4>
                    <h3 class="text-3xl lg:text-4xl font-bold text-green-500 leading-none pt-2 after-hero-link-title">{{ item.title }}<img class="hidden xl:inline pl-3 after-hero-link-arrow" src="https://inside.bullochacademy.com/wp-content/uploads/2021/09/icon_arrow.png"></img></h3>
                </a>
            </div>
        {% endfor %}
    </div>
</div>

<style>
    .gator-cta-tranparent-head {
        background-image: url('https://inside.bullochacademy.com/wp-content/uploads/2021/09/gator-transparent.png');
    }
    .gator-video {
        min-height: 382px;
        border-radius: 6px;
    }
</style>

<div class="relative bg-gold-50 gator-cta">
    <div class="w-full h-full bg-cover xxxl:bg-contain bg-center bg-no-repeat py-20 px-2 sm:px-6 md:px-10 lg:px-16 gator-cta-tranparent-head">
        <div class="flex flex-wrap items-center flex-col-reverse md:flex-row max-w-8xl mx-auto xxxl:px-12">
            <div class="md:w-1/2 md:pr-20">
                <h2 class="text-3xl lg:text-h2-init pb-2 leading-none">{{ homePage.gatorCtaTitle }}</h2>
                <p class="text-base lg:text-p-init pb-6">{{ homePage.gatorCtaParagraph }}</p>
                <a {% if homePage.gatorCtaButtonLinkDestination == "A page on this site" %}href='{{ homePage.gatorCtaButtonLinkOnSite }}'{% elseif homePage.gatorCtaButtonLinkDestination == "Another site" %}href='{{ homePage.gatorCtaButtonLinkOffSite }}' target='_blank'{% endif %}>
                    <button class="arrow-button">{{ homePage.gatorCtaButtonText }}</button>
                </a>
            </div>
            <iframe class="w-full md:w-1/2 pb-12 md:pb-0 gator-video" src="{{ homePage.gatorCtaVideoLink | replace('youtu.be/', 'www.youtube.com/embed/')}}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>
    </div>
</div>

